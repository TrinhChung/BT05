#include<iostream>
#include<ctime>
#include<stdlib.h>
using namespace std;
int** creatMatrix(int m,int n){
    int** a;
    a=new int*[m+2];
    for(int i=0;i<m+2;i++){
        a[i]=new int[n+2];
    }
    for(int i=0;i<m+2;i++){
        for(int j=0;j<n+2;j++){
            *(*(a+i)+j)=0;
        }
    }
    return a;
}
void tongBom(int** a,int m,int n,int& nobom){
    for(int i=1;i<=m;i++){
        for(int j=1;j<=n;j++){
            if(a[i][j]!=10){nobom+=a[i][j];}
        }
    }
}
void randBoom(int** a,int m,int n,int k){
    srand(time(NULL));
    int bom=k;
    while(bom>0){
        int x=rand()%m+1;
        int y=rand()%n+1;
        if(a[x][y]!=10){
            a[x][y]=10;
            bom--;
        }
    }
}
void demBom(int **a,int m,int n){
    for(int i=1;i<=m;i++){
        for(int j=1;j<=n;j++){
            if(a[i][j]!=10){
                if(a[i-1][j]==10){a[i][j]++;}
                if(a[i-1][j-1]==10){a[i][j]++;}
                if(a[i-1][j+1]==10){a[i][j]++;}
                if(a[i][j-1]==10){a[i][j]++;}
                if(a[i][j+1]==10){a[i][j]++;}
                if(a[i+1][j]==10){a[i][j]++;}
                if(a[i+1][j-1]==10){a[i][j]++;}
                if(a[i+1][j+1]==10){a[i][j]++;}
            }
        }
    }
}
void printMatrix(int** a,int m,int n){
    for(int i=1;i<=m;i++){
            if(i>1) cout <<endl;
        for(int j=1;j<=n;j++){
            if(a[i][j]<10){ cout <<" "<<a[i][j]<<" ";}
            else cout <<a[i][j]<<" ";
        }
    }
}
void playBom(int** a,int** b,int& t,int noBom,int m,int n){
    bool check=true;
    while(check){
        int x,y;
        bool in=false;
        while(!in){
            cout <<"Nhap toa do x[1,"<<m<<"]: ";
            cin >>x;
            cout <<endl;
            if(x<1 || x>m){
                cout <<"Nhap x nho hon "<<m<<" : ";
                cin  >>x;
                cout <<endl;
            }
            cout <<"Nhap toa do y[1,"<<n<<"]: ";
            cin >>y;
            cout <<endl;
            if(y<1 || y>n){
                cout <<"Nhap y nho hon "<<n<<" : ";
                cin  >>x;
                cout <<endl;
            }
            if(x>=1 && x<=m && y>=1 && y<=m){
                in=true;
            }
        }
        if(a[x][y]!=10){
            t+=a[x][y];
            b[x][y]=a[x][y];
            printMatrix(b,m,n);
        }
        if(t==noBom){
                cout <<"You WIn";
                break;}
        if(a[x][y]==10){
            cout <<"YOUâ€™RE DEAD!"<<endl;
            check=false;
            break;
        }
    }
}
void destroy(int** a,int m,int n){
    for(int i=0;i<m;i++){
        delete []a[i];
    }
    a=NULL;
}
int main(){
    int m,n,k;
    cin >>m>>n>>k;
    int **a=creatMatrix(m,n);
    int **b=creatMatrix(m,n);
    randBoom(a,m,n,k);
    demBom(a,m,n);
    int noBom=0;
    int t=0;
    tongBom(a,m,n,noBom);
    playBom(a,b,t,noBom,m,n);
    printMatrix(a,m,n);
    destroy(a,m,n);
}
