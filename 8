#include<iostream>
using namespace std;
int roundNummer(double n){
    int a=n;
    int b=a+1;
    if(n-a>b-n) return b;
    else{
        if(n-a==b-n){
            return a;
        }
        else{
            return a;
        }
    }
}
int main(){
    double n;
    cin >>n;
    cout <<roundNummer(n);
}
